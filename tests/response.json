            "id": "3b4aa1a9-9b67-474f-ad4f-d210f33c7f08",
            "is_public": false,
            "quote": "Leaders cannot possibly detect all risks from a single vantage point, so engaging with others through execution is essential to expanding their awareness.",
            "ranges": [
                {
                    "end": "/p[9]",
                    "endOffset": 437,
                    "start": "/p[9]",
                    "startOffset": 283
                }
            ],
            "text": "",
